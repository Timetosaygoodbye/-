.container-main {
  display: flex;
  flex-direction: column;
  background: #de3525;
  height: 100%;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.title {
  display: flex;
  flex-direction: row;
  background: #de3525;
  height: auto;
  width: 100%;
  align-items: center;
  justify-content: space-between;
  margin: 0;
  padding: 5rpx;
  box-sizing: border-box;
}

.body {
  display: flex;
  flex-direction: column;
  background: white;
  height: auto;
  width: 99%;
  align-items: center;
  justify-content: space-between;
  margin: 0 auto;
  box-sizing: border-box;
  border-radius: 25rpx 25rpx 0 0;
}

.avatar {
  overflow: hidden;
  width: 100rpx;
  height: 100rpx;
  margin: 50rpx 0 50rpx 50rpx;
  border-radius: 50%;
  border: 1rpx solid white;
}

.HongKongImage {
  width: 450rpx;
  height: 250rpx;
}

.couponImageSuper {
  width: 673rpx;
  height: 320rpx;
  box-shadow: 0rpx 5rpx 20rpx rgba(128, 128, 128, 0.3);
  margin: 20rpx 0;
}

.couponImageMoreReduction {
  width: 673rpx;
  height: 334rpx;
  box-shadow: 0rpx 5rpx 20rpx rgba(128, 128, 128, 0.3);
  margin: 20rpx 0;
}

.couponImageReceived {
  width: 673rpx;
  height: 265rpx;
  box-shadow: 0rpx 5rpx 20rpx rgba(128, 128, 128, 0.3);
  margin: 20rpx 0;
}

.smallCouponImage {
  width: 317rpx;
  height: 410rpx;
  box-shadow: 0rpx 5rpx 20rpx rgba(128, 128, 128, 0.3);
  margin: 20rpx 0;
}

.nameArea {
  display: flex;
  flex-direction: column;
  padding: 0;
  align-items: center;
}

.chineseName {
  font-size: 35rpx;
  color: white
}

.englishName {
  font-size: 28rpx;
  color: rgba(255, 255, 255, 0.3)
}

.startSign {
  height: 30rpx;
  border: 5rpx solid #de3525;
  margin: 10rpx;
  overflow: hidden;
}

.couponModule {
  display: flex;
  flex-direction: column;
  background: white;
  height: auto;
  width: 100%;
  align-items: center;
  justify-content: space-between;
  margin: 20rpx 0;
  box-sizing: border-box;
}

.smallCouponModule {
  display: flex;
  flex-direction: row;
  background: white;
  height: auto;
  width: 90%;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  margin: 5rpx 0;
}

.couponModuleTitle {
  display: flex;
  flex-direction: row;
  background: white;
  height: auto;
  width: 85%;
  position: relative;
  justify-content: space-between;
  align-items: center;
  margin: 30rpx;
}

.couponModuleTitle:before {
  content: '';
  height: 30rpx;
  border: 5rpx solid #de3525;
  position: absolute;
  left: -15rpx;
  top: 5rpx;
}

.couponModuleTitlefront {
  font-size: 30rpx;
  color: black;
}

.couponModuleTitleback {
  font-size: 25rpx;
  color: gray;
}

.moreStores {
  display: flex;
  flex-direction: column;
  background: white;
  height: auto;
  width: 100%;
  align-items: center;
  justify-content: space-between;
  margin: 40rpx 0;
  padding: 20rpx 20rpx;
  box-sizing: border-box;
}

.moreStoreImage {
  width: 673rpx;
  height: 140rpx;
  box-shadow: 0rpx 5rpx 20rpx rgba(128, 128, 128, 0.3);
}

.myfooter {
  display: flex;
  flex-direction: column;
  background: white;
  height: auto;
  width: 100%;
  align-items: center;
  justify-content: space-between;
  margin: 10rpx 0 80rpx 0;
  padding: 0 0 80rpx 0;
  box-sizing: border-box;
}

.myTabbar {
  z-index: 100;
  width: 100%;
  height: 140rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: row;
  background: white;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 30rpx 30rpx;
  color: gray;
}

.moneyIcon {
  width: 46rpx;
  height: 41rpx;
}

.payIcon {
  width: 270rpx;
  height: 88rpx;
}

.arrowIcon {
  width: 16rpx;
  height: 28rpx;
  top: 6rpx;
  position: relative;
}

.tabbarLabel {
  align-items: center;
  font-size: 25rpx;
}

.img-button {
  padding: 0;
  border: none;
  background-color: transparent;
}

.myMask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: black;
  z-index: 9000;
  opacity: 0.5;
}

.myDialog {
  width: 70%;
  height: auto;
  position: fixed;
  top: 220rpx;
  left: 0;
  right: 0;
  z-index: 10000;
  margin: 0 auto;
  background-color: transparent;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.myDialogText {
  color: white;
  font-size: 40rpx;
  width: 100%;
  height: auto;
  align-items: flex-start;
  display: flex;
  justify-content: start;
}

.DialogImage {
  width: 612rpx;
  height: 776rpx;
  margin: 10rpx 0 85rpx 0;
  z-index: 11000;
}

.DialogBackgroundImage {
  width: 130rpx;
  height: 180rpx;
  z-index: 10050;
  position: absolute;
  top: 300rpx;
}
.scrollBarContainer {
  width: 130rpx;
  height: 180rpx;
  position: absolute;
  top: 300rpx;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}

.scrollBar {
  width: auto;
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  z-index: 10100;
  position: absolute;
  top: 0;
}

.DialogBackgroundText {
  color: white;
  height: 200rpx;
  width: auto;
}
.scrollText{
  font-size: 130rpx;
}

.myDialogButton {
  width: 90% !important;
}